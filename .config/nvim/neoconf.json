{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": [
        "neoconf.nvim",
        "nvim-lspconfig",
        "lsp"
      ]
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.completion.callSnippet": "Replace",
      "Lua.window.progressBar": false,
      "Lua.window.statusBar": false
    }
  }
}
